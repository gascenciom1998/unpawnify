{
  "manifest_version": 3,
  "description": "Chrome extension to protect reused passwords",
  "name": "Unpawnify",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.6.0.min.js", "content.js"]
    }
  ],
  "action": {
    "default_icon": "pawn.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}